{
  "name": "byteleaf-ui-react",
  "version": "2.0.3",
  "author": "byteleaf GmbH",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "build-and-publish": "rimraf ./lib && tsc -p tsconfig-build.json && cp package.json ./lib/package.json && cp README.md ./lib/README.md && npm publish lib",
    "test": "cross-env CI=true react-scripts test --coverage",
    "test:update": "cross-env CI=true react-scripts test -u",
    "test:watch": "react-scripts test",
    "lint": "eslint src --ext .js,.ts,.jsx,.tsx",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.5.0",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addon-notes": "^5.2.5",
    "@storybook/addon-storyshots": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.144",
    "@types/node": "^12.11.5",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "lodash": "^4.17.15",
    "prettier": "^1.18.2",
    "react": "^16.11.0",
    "react-app-polyfill": "^1.0.5",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0",
    "react-test-renderer": "^16.11.0",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.3"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.5.0",
    "lodash": "^4.17.15",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{ts,tsx}",
      "!src/components/**/*.stories.{ts,tsx}"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  },
  "dependencies": {
    "material-ui-dropzone": "^2.4.7",
    "node-sass": "^7.0.0"
  }
}
